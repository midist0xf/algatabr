<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.HBox?>

<HBox alignment="CENTER_LEFT" prefHeight="500.0" prefWidth="1060.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="controller.LessonController">
  <!-- NB Quando si vogliono usare i controller nella classe lesson, chiamali nome+"controller" 
  	   per esempio questionController
  -->
  <children>
    <Pane prefHeight="200.0" prefWidth="260.0">
      <children>
        <fx:include fx:id="pseudocode" source="PseudocodeView.fxml" />
      </children>
    </Pane>
    <Pane prefHeight="200.0" prefWidth="550.0">
      <children>
        <fx:include fx:id="control" source="ControlBarView.fxml" />
      </children>
    </Pane>
    <Pane prefHeight="200.0" prefWidth="250.0">
      <children>
        <fx:include fx:id="question" source="QuestionView.fxml" />
      </children>
    </Pane>
  </children>
</HBox>
